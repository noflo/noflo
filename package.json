{
  "name": "noflo",
  "description": "Flow-Based Programming environment for JavaScript",
  "keywords": [
    "fbp",
    "workflow",
    "flow",
    "noflo"
  ],
  "author": "Henri Bergius <henri.bergius@iki.fi>",
  "version": "1.1.3",
  "license": "MIT",
  "engines": {
    "node": ">= 6"
  },
  "dependencies": {
    "coffeescript": "^2.2.1",
    "debug": "^4.0.1",
    "fbp": "^1.5.0",
    "fbp-graph": "^0.4.0",
    "fbp-manifest": "^0.2.0",
    "get-function-params": "^2.0.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "chai": "^4.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "karma": "^5.1.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "mocha": "^5.0.0",
    "noflo-component-loader": "^0.3.2",
    "nyc": "^14.1.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "main": "./lib/NoFlo",
  "bin": {
    "noflo": "./bin/noflo",
    "noflo-cache-preheat": "./bin/noflo-cache-preheat"
  },
  "homepage": "http://noflojs.org/",
  "repository": {
    "type": "git",
    "url": "git://github.com/noflo/noflo.git"
  },
  "scripts": {
    "pretest": "eslint lib",
    "test:node": "nyc mocha spec",
    "test:browser": "karma start karma.config.js",
    "test": "npm run test:node && npm run test:browser",
    "build:node": "babel src --out-dir .",
    "build:browser": "webpack",
    "build": "npm run build:node && npm run build:browser"
  },
  "docco_husky": {
    "output_dir": "docs",
    "project_name": "NoFlo"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ]
  }
}
